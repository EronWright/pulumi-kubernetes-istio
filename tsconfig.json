{
  "compilerOptions": {
    "outDir": "bin",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["esnext"],
    "declaration": true,
    "sourceMap": false,
    "strict": true,
    "stripInternal": true,
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "pretty": true,
    "forceConsistentCasingInFileNames": true
  }
}